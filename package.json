{
  "name": "duckshop",
  "version": "0.1.0",
  "description": "Rubber duck shop",
  "main": "index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "clean": "rimraf dist/**",
    "format": "prettier --config .prettierrc --write ./src",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "copy:static": "mkdir -p dist && cp -R src/static dist/static",
    "build": "tsc --project tsconfig.json",
    "dev": "nodemon --config nodemon.json src/index.ts",
    "start:dev2": "nodemon --config nodemon.json --exec \"npm run build && npm run start\"",
    "start:dev": "tsnd src/index.ts",
    "build:prod": "npm run clean && npm run copy:static && npm run build",
    "test": "jest"
  },
  "keywords": [],
  "author": "Maciej Szeremeta",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/morgan": "^1.9.3",
    "@types/mssql": "^8.0.2",
    "@types/node": "^17.0.42",
    "@types/nodemon": "^1.19.1",
    "@types/prettier": "^2.6.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.5.0",
    "i": "^0.3.7",
    "nodemon": "^2.0.16",
    "npm": "^8.13.1",
    "prettier": "^2.7.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.4.0",
    "helmet": "^5.1.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "uuid": "^8.3.2"
  }
}
